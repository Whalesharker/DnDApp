<!--Currently this is the only page where the default layout just doesn't happen.-->
<table class="table table-bordered table-striped">
    <thead>
        <th>name</th>
        <th>desc</th>
        <th>damage type </th>
    </thead>
    <tbody>
        <% for(s of spells){%> 
            <tr> 
                <td> <%= s.name %> </td> 
                <td> <%= s.desc %> </td> 
                <td> <%= s.damage_type %> </td>

            </tr>
        <% } %>
    </tbody>
</table>
<%= JSON.stringify(spells) %>